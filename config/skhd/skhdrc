lctrl - 1 : yabai -m space --focus 1
lctrl - 2 : yabai -m space --focus 2
lctrl - 3 : yabai -m space --focus 3
lctrl - 4 : yabai -m space --focus 4
lctrl - 5 : yabai -m space --focus 5
lctrl - 6 : yabai -m space --focus 6
cmd + lctrl - 1 : yabai -m window --space 1
cmd + lctrl - 2 : yabai -m window --space 2
cmd + lctrl - 3 : yabai -m window --space 3
cmd + lctrl - 4 : yabai -m window --space 4
cmd + lctrl - 5 : yabai -m window --space 5
lctrl + alt - f : yabai -m window --toggle zoom-fullscreen
cmd - b : open -a "Brave Browser" -n
cmd + alt - t : open -a "kitty" -n --args --single-instance

alt - 1 : yabai -m space --destroy 1
alt - 2 : yabai -m space --destroy 2
alt - 3 : yabai -m space --destroy 3
alt - 4 : yabai -m space --destroy 4
alt - 5 : yabai -m space --destroy 5

lctrl + alt - c : /Users/ridham.zalawadia/.config/yabai/cycle_clockwise.sh

lctrl + alt - e : yabai -m window --focus next || yabai -m window --focus first
